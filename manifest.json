{
  "manifest_version": 3,
  "name": "GitHub Reverse Branches",
  "version": "1.0.0",
  "description": "Allows to easilt reverse github branches when creating a pull request",
  "permissions": ["tabs"],
  "action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}